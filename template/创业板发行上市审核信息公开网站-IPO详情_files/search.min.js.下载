$(function(){var keyReplaceMap={"%":"%25","#":"%23","&":"%26","=":"%3D"};$(".project-search-btn").on("click",function(){var keywords=$("input[name=projectKeywords]").val();for(key in keyReplaceMap)if(keyReplaceMap.hasOwnProperty(key)){var reg="/"+key+"/g";keywords=keywords.replace(eval(reg),keyReplaceMap[key])}window.location.href=pathObj.ras_domain_path_http+"/projectdynamic/ipo/index.html?keywords="+keywords}),$(".search-input").keypress(function(e){(e=event||window.event||arguments.callee.caller.arguments[0])&&13==e.keyCode&&$(".project-search-btn").trigger("click")})});